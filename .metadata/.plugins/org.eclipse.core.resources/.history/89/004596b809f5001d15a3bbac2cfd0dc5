package Desafios;

import java.util.Scanner;

public class WhileEstudo {

	public static void main(String[] args) {

		Scanner sc = new Scanner(System.in);
		System.out.println("Digite a nota: ");
		
		int nota = 0;
		int total;
		int quantidadeDeNotas = 0;
		if(nota > 1 && nota < 10) {
			total =+ nota;
			quantidadeDeNotas++;
		}
		
		
		
		
		
		sc.close();
	}

}
